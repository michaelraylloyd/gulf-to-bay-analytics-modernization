	measure 'Repeat Customers' =
			
			CALCULATE (
			    DISTINCTCOUNT ( FactInternetSales[CustomerKey] ),
			    FILTER (
			        VALUES ( FactInternetSales[CustomerKey] ),
			        CALCULATE (
			            COUNTROWS ( FactInternetSales ),
			            ALLEXCEPT ( FactInternetSales, FactInternetSales[CustomerKey] )
			        ) > 1
			    )
			)
		formatString: #,0
		lineageTag: 94ee0227-3183-402d-b3d1-cdd6d706679f

	measure 'Top Category' =
			
			CALCULATE(
			    FIRSTNONBLANK( 'DimProductCategory'[Category], 1 ),
			    TOPN(
			        1,
			        SUMMARIZE(
			            'DimProduct',
			            'DimProductCategory'[Category],
			            "Sales", [Total Sales]
			        ),
			        [Sales],
			        DESC
			    )
			)
		lineageTag: 9234d241-5054-46b6-8a08-9ccf6273748b

	measure 'Dynamic Subtitle Orders' =
			
			VAR Pct =
			    FORMAT( [Orders % to Goal], "0%" )
			
			VAR Category =
			    [Top Category]
			
			RETURN
			"Orders are " & Pct & " to goal, led by " & LOWER(Category) & " orders."
		lineageTag: f1f0c009-dccc-44e3-8b50-71c0330f3727

	measure 'Dynamic Subtitle Sales' =
			
			VAR Pct =
			    FORMAT( [Sales % to Goal], "0%" )
			
